{ 
  "version": 2, 
  "builds": [ 
    { 
      "src": "frontend/package.json", 
      "use": "@vercel/static-build", 
      "config": { 
        "distDir": "build", 
        "framework": "create-react-app" 
      } 
    } 
  ], 
  "routes": [ 
    { 
      "src": "/(.*)", 
      "dest": "/$1" 
    } 
  ] 
} 
